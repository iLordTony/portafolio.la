@font-face
   font-family 'icomoon';
   src url('../fonts/icomoon.eot?n2stke');
   src:url('../fonts/icomoon.eot?#iefixn2stke') format('embedded-opentype'),
      url('../fonts/icomoon.woff?n2stke') format('woff'),
      url('../fonts/icomoon.ttf?n2stke') format('truetype'),
      url('../fonts/icomoon.svg?n2stke#icomoon') format('svg');
   font-weight normal
   font-style normal

[class^="icon-"], [class*=" icon-"]
   font-family 'icomoon'
   speak none
   font-style normal
   font-weight normal
   font-variant normal
   text-transform none
   line-height 1
   -webkit-font-smoothing antialiased
   -moz-osx-font-smoothing grayscale

.icon-icon-face
   &:before
      content "\ea8d"

.icon-icon-twitter
   &:before
      content "\ea91"

.icon-icon-git
   &:before
      content "\e602"

